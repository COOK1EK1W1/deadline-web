datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Deadline {
  name    String
  subject String
  start   DateTime?
  due     DateTime
  mark    Float?
  room    String?
  url     String?
  info    String?
  color   Int?
  id      Int       @id @default(autoincrement())
}
