datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Deadline{
  name String 
  subject String
  start DateTime?
  due DateTime
  mark Float?
  room String?
  url String?
  info String?
  color Int?
  
  @@id([name, subject])
}